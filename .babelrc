{
    "presets": [
        "env",
        "react",
        "stage-0"
    ],
    "plugins": [
        "transform-runtime",
        [
            "import", 
            { "libraryName": "antd", "libraryDirectory": "es", "style": "css" } // `style: true` 会加载 less 文件
        ],
        [
            "import",
            {
                "libraryName": "@/components",
                "camel2UnderlineComponentName": false,
                "camel2DashComponentName": false
            },
            "components"
        ],
        ["transform-decorators-legacy"]
    ]
}

